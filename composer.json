{
    "name": "mitchfizz05/sphinx",
    "description": "Run your own Minecraft Realms endpoint!",
    "keywords": ["minecraft", "server", "realms"],
    "license": "MIT",
    "type": "project",
	"minimum-stability": "dev",
	"prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "vluzrmos/tinker": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
